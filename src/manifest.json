{
  "name": "clear-localhost",
  "version": "0.1",
  "description": "Clear cache on localhost reload.",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["http://localhost/*", "tabs", "browsingData", "storage"]
}
